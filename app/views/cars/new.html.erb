<h1>Add new vehicle</h1>
<%= form_for @car do |f|%>
   <% if @car.errors.any? %>
    <h2>Errors:</h2>
      <ul>
       <% @car.errors.full_messages.each do |message| %>
       <li><%= message %></li>
      <% end %> 
     </ul>
    <% end %>
  <%= f.label :manaufacturer %><br>
  <%= f.text_field :manufacturer%><br>

  <%= f.label :model %><br>
  <%= f.text_field :model%><br>

  <%= f.label :year %><br>
  <%= f.text_field :year%><br>

 <%= f.submit 'save'%>
 <% end %>
